page{
  background: #f8f8f8;
}
.title{
  background: #ebf9eb;
  padding: 30rpx 0;
  color: #00B402;
  font-size: 28rpx;
}
form .form-item {
  display: flex;
  padding:16rpx 0;
  margin-bottom: 10rpx;
  font-size:24rpx;
  background: #fff;
  padding-bottom:8px;
}
form .form-item .wapper-30 .form-title > text{
  color: #FE3A3B;
  margin-right: 8rpx;
}
form .form-item .form-flex{
  display: flex;
}
form .form-item .site image{
  width: 13rpx;
  height: 22rpx;
  margin-left: auto;
}
form .form-item .site{
  width: 100%;
}
.form-item-detailed-address{
  display: flex;
  align-items: center;
  background: #fff;
  font-size: 28rpx;
}
.form-item-detailed-address .form-title{
  padding-top: 62rpx;
  padding-left: 30rpx;
  padding-right: 25rpx;
  padding-bottom: 61rpx;
}
.form-item-detailed-address .form-title > text{
  color: #FE3A3B;
  margin-right: 8rpx;
}
.form-item-detailed-address textarea{
  height: 100rpx;
  font-size: 24rpx;
  padding-top: 16rpx;
  padding-left: 9rpx;
  border: 1rpx solid rgba(231,231,231,1);
  border-radius: 4rpx;
  margin-right: 20px;
  padding: 10px;  
  border-radius:10rpx;
}
form .upload-box{
  background: #fff;
  padding: 0 30rpx;
  margin-bottom: 20rpx;
}
form .upload-box .images-box{
  display: flex;
  justify-content: space-between;
  padding: 32rpx 0;
}
form .upload-box .images-box .id-card{
  width:332rpx;
  height:208rpx;
  background:rgba(248,248,248,1);
  border-radius:10rpx;
  overflow: hidden;
}
.ico21pic{width:30rpx; height:30rpx; position:absolute; top:1rpx; right:5rpx;z-index: 999}
form .upload-box .images-box .id-card image{
  width: 100%;
  height: 100%;
}
form .upload-box .id-card-title{
  display: flex;
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 43rpx;
}
form .upload-box .id-card-title view{
  width: 50%;
}
form .form-post{
  background: #fff;
  padding-bottom: 62rpx;
}
form .form-post .agreement{
  display: flex;
  padding: 35rpx 0;
  font-size: 24rpx;
  font-weight:bold;
  color: #333333;
  background: #fff;
}
form .form-post .agreement radio .wx-radio-input{
  width: 25rpx;
  height: 25rpx;
  padding: 0;
  margin-right: 8rpx;
  margin-top: -5rpx;
  background: #fff;
  border: 1rpx solid #ccc;
  border-radius:3rpx;
}
form .form-post .agreement radio .wx-radio-input.wx-radio-input-checked::before{
  background: #00B402;
  color: #fff;
  border: 0;
  border-radius:3rpx;
}
form .form-post text{
  color: #599cff;
}
