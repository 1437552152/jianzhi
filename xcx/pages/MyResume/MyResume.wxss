.ok{
  color: #fff;
  height: 88rpx!important;
  line-height: 88rpx!important;
  text-align: center;
  border-radius: 44rpx;
  background-color: #53B2AA;
  width: 610rpx!important;
  margin-top: 30rpx;
  margin-left:auto;
  margin-right:auto;
  font-size: 30rpx;
  padding: 0;
}

.address-picker{
  width: 100%;
  line-height: 55rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.picker-view{
  width: 525rpx;
  display: flex;
  position: relative;
}
.picker-view image{
  position: absolute;
  right: 0;
  top: 16.5rpx;
}
.takephoto{
  background-color: #fff;
  padding-left: 20px;
}
.takephoto image{
  margin-top: 32rpx;
  height: 200rpx;
  margin-bottom: 16px;
  width: 200rpx;   
}

.title_leftImage {
width: 8rpx;
height: 33rpx;
}

.common_title{
  margin-left:20rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
}
.common_title{display: flex;justify-content: space-between;align-items: center;position: sticky;top:0;background-color: #f8f8f8;
  left: 0;
  z-index: 999;
  }
.common_title .title_left{display: flex;align-items: center;}
.title_left .txt{
font-size:24rpx;
color: #333;
font-weight: normal;
margin-left: 4px;
}

.title_left image{width: 8rpx;height: 33rpx;}

/* 布局容器，左右边距30rpx */
.wapper-30{
  padding: 0 30rpx;
  box-sizing: border-box;
}

