margin-bottom: 10px;
}


.baomingStatus{margin: 20px 10px;
  border-radius: 10px;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  padding:20px 10px;
  
}

.baomingStatus .list {
  flex:1;
 text-align: center;
 position: relative;
}

.baomingStatus .list .num{
  color:#000;
  font-weight: bold;
}
.baomingStatus .list .desc{
  color: #999;
  font-size: 14px;
  margin-top: 4px;  
}

.baomingStatus .list .line{
  width: 1px;
height:30px;
background-color: #f7f5f5;
position: absolute;
right: 0;
top: 10px;
}
.headtopBg{
  background-color: #5a99fb;
  height: 80px;
}
.tixianBeizhu{
  text-align: right;
  margin-top: 6px;
  margin-bottom: 6px;
  color: #9a9c9e;
  margin-right: 18px;  
}
.card{
  margin: 0 16px;
font-size: 14px;
overflow: hidden;
position: relative;
background: #fff;
border: 1rpx solid #dddee1;
border-radius: 5px;

}
.card .item{
  display: flex;
  justify-content: space-between; 
  font-size: 13px;
}
.card .item:first-child{
  border-bottom: 1px solid #eee;
}
.card .item view{
  padding:12px 8px;
}
.input{
  border: 1px solid #eee;
margin: 20px 10px;
padding: 6px;
border-radius: 4px;
margin-bottom: 0px;
font-size: 16px;
}