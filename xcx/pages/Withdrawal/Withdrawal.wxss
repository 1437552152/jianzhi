  line-height: 35px;

  /* padding: 10px; */
}

.part{
  width: 94%;
  margin: 20rpx auto;
  border-radius:16rpx;
border:1px solid #fff;
}
.personalinfo{
 display:flex;
justify-content:space-between;
padding-left:60rpx;
padding-right:60rpx;
align-items:center;
position:absolute;
top:70rpx;
left:0;
right:0;
}
.left_part{
  display: flex;
  justify-content: space-between;
}
.one image{
  width: 100rpx;
  height: 100rpx;  
  vertical-align: middle;
  border-radius: 50%;
} 

.orderImg{
  width: 70rpx;
  height: 60rpx;
}


.two{
  margin-left: 27rpx;
}
.two .name{
color: #ffffff;
font-size: 32rpx;
}
.two .num{
  color: #ffffff;
  font-size: 26rpx;
  margin-top: 23rpx;
}
.two .num text{
  font-size: 40rpx;
  margin-right:20rpx;
}
.three{
  margin-left: 16rpx;
  background-color: rgba(255, 255, 255, 0.3);
  color: #fff;
  text-align: center;
  line-height: 48rpx;
  height: 48rpx;
  /* width: 100rpx; */
  border-radius: 24rpx;
  font-size: 24rpx;
  padding: 0rpx 30rpx;
}
.right_part image{
  width: 14rpx;
